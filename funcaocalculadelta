#include <stdio.h>
#include <string.h>
#include <ctype.h>
#include <stdlib.h>
#include <tgmath.h>
double delta(double a, double b, double c){
	double d = pow(b, 2) - (4 * a * c);
	return d;
}
int main(){
	double a, b, c;
	scanf("%lf %lf %lf", &a, &b, &c);
	double d = delta(a, b, c);
	printf("%.1lf\n", d);

	return 0;
}
/*Crie uma função capaz de receber 3 números 
a, b e c, e retornar o delta desses números.*/
